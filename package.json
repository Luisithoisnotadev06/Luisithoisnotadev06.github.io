{
	"name": "pianotiles2-wiki",
	"private": true,
	"main": "index.js",
	"license": "MIT",
	"devDependencies": {
		"prettier": "^3.2.5",
		"vitepress": "^1.0.1"
	},
	"scripts": {
		"wiki:dev": "vitepress dev wiki",
		"wiki:build": "vitepress build wiki",
		"wiki:preview": "vitepress preview wiki",
		"format": "prettier --write ."
	},
	"prettier": {
		"arrowParens": "avoid",
		"endOfLine": "crlf",
		"printWidth": 120,
		"proseWrap": "never",
		"semi": true,
		"singleQuote": true,
		"tabWidth": 4,
		"trailingComma": "all",
		"useTabs": true
	}
}
